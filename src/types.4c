#include "std/core.4c"

struct File {
    ptr name
}

struct ScopedKey {
    ptr name
    int scope
}

struct Buffer {
    ScopedKey key
    int size
    ptr init
    bool init_int
}

struct Macro {
    ScopedKey key
    ptr toks
}

struct Function {
    ptr name
    ptr def
}

struct Struct {
    ptr name
    ptr fields
}

struct StructField {
    ptr name
    int size
}

struct Op {
    char kind
    ptr data
}

struct Token {
    char kind
    ptr data
}