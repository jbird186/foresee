#include "std/linux.5th"

#%{over over / * -}

#false{0}
#true{1}
#bool{(if {true} else {false})}
#not{(if {false} else {true})}
#!={== 1 ^}
#<={> 1 ^}
#>={< 1 ^}
#is_success{-2 !=}
#is_fail{-2 ==}

#dup{0 pick}
#over{1 pick}
#swap{1 roll}
#rot{2 roll}

#dowhile(__ops __cond) {
    __ops
    while __cond {
        __ops
    }
}

#for(__init __cond __iter __ops) {
    __init
    while __cond {
        __ops
        __iter
    }
}