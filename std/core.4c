#include "std/linux.4c"

#CHAR{1}
#INT{8}
#REF{8}

#%{over over / * -}

#NULL{0}

#false{0}
#true{1}
#bool{(if {true} else {false})}
#not{(if {false} else {true})}
#!={== 1 ^}
#<={> 1 ^}
#>={< 1 ^}
#is_success{0 >}
#is_failure{0 <}

// a --- a a
:dup{0 pick}
// a b --- a b a
:over{1 pick}
// a b --- b a
:swap{1 roll}
// a b c --- b c a
:rot{2 roll}

#dowhile(__ops __cond) {
    __ops
    while __cond {
        __ops
    }
}